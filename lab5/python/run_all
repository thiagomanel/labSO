#/bin/bash

# This script exercises the real workload
npages=1500000
clock=10

for trace in trace.1 trace.2
do
    #for alg in fifo nru aging second-chance
    for alg in fifo
    do
        for nframes in 4 8 16 32
        do
	    python memsym.py $npages $nframes $alg $clock < load/$trace > output/$trace.$alg.$nframes.out
	done
    done
done
